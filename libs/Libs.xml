<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
	<!-- LibStub must always be loaded first -->
	<Script file="libs\LibStub\LibStub.lua"/>
	
	<!-- CallbackHandler is needed by many Ace3 libraries -->
	<Script file="libs\CallbackHandler-1.0\CallbackHandler-1.0.lua"/>
	
	<!-- Ace3 Core Libraries (load in dependency order) -->
	<Script file="libs\AceAddon-3.0\AceAddon-3.0.lua"/>
	<Script file="libs\AceDB-3.0\AceDB-3.0.lua"/>
	<Script file="libs\AceSerializer-3.0\AceSerializer-3.0.lua"/>
	<Script file="libs\AceDBOptions-3.0\AceDBOptions-3.0.lua"/>
	<Script file="libs\AceGUI-3.0\AceGUI-3.0.lua"/>
	<Script file="libs\AceHook-3.0\AceHook-3.0.lua"/>
	<Script file="libs\AceEvent-3.0\AceEvent-3.0.lua"/>
	<Script file="libs\AceTimer-3.0\AceTimer-3.0.lua"/>
	<Script file="libs\AceConsole-3.0\AceConsole-3.0.lua"/>
	<Script file="libs\AceLocale-3.0\AceLocale-3.0.lua"/>
	<Script file="libs\AceConfig-3.0\AceConfig-3.0.lua"/>
	<Script file="libs\AceConfigDialog-3.0\AceConfigDialog-3.0.lua"/>
	<Script file="libs\AceConfigRegistry-3.0\AceConfigRegistry-3.0.lua"/>
	
	<!-- Other utility libraries -->
	<Include file="libs\LibSerialize\lib.xml"/>
	<Include file="libs\LibDeflate\lib.xml"/>
	<Include file="libs\LibCustomGlow-1.0\LibCustomGlow-1.0.xml"/>
	
	<!-- AbstractFramework (core framework library) -->
	<Include file="libs\AbstractFramework\lib.xml"/>
	
	<!-- OpenRaidLibrary (raid data sharing library) -->
	<Include file="libs\OpenRaidLibrary\lib.xml"/>
	
	<!-- oUF (unit frame library) - must use XML to load in correct order -->
	<Include file="libs\oUF\oUF.xml"/>
	
	<!-- Additional libraries -->
	<Script file="libs\LibDualSpec-1.0\LibDualSpec-1.0.lua"/>

    	<!-- LibAnchorRegistry-1.0 (inter-addon frame anchoring system) -->
	<Script file="libs\LibAnchorRegistry-1.0\LibAnchorRegistry-1.0.lua"/>
	
	<!-- LibDogTag-3.0 (base library for dynamic text tags) -->
	<Include file="libs\LibDogTag-3.0\lib.xml"/>
	
	<!-- LibDogTag-Unit-3.0 (unit extension, depends on LibDogTag-3.0) -->
	<Include file="libs\LibDogTag-Unit-3.0\lib.xml"/>
</Ui>
